#include<stdio.h>

int
main ()
{
  int bilanganarr[150];
  int *point = &bilanganarr[0];
  for (int i = 0; i < 150; i++)
    {
      bilanganarr[i] = i + 1;

    }
  for (int i = 1; i <= 150; i++)
    {
      printf ("%i\n", *point=i);
    }


  return 0;
}


