#include<stdio.h>

int main()
{
    int bil[150];
    int* pointbil = &bil[0];
    
    for(int i=0;i<150;i++){
        bil[i] = i + 1;
        printf("%i\n",*pointbil);
        pointbil++;
    }
    
return 0;
}
